<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd"	
	icon="user-frontoffice" editable="true" publishable="true" >
	<properties>
		<property name="label">
			<constraint name="pseudonym" />
		</property>
		<property name="email" type="String" required="true">
			<constraint name="email" />
		</property>
		<property name="isroot" type="Boolean" default-value="false" />
		<property name="issudoer" type="Boolean" default-value="false" />
		<property name="login" type="String" required="true">
			<constraint name="login" />
		</property>
		<property name="passwordmd5" type="String" indexed="none" required="true">
			<constraint name="maxSize" max="50" />
		</property>
		<property name="groups" type="DocumentArray" document-type="Change_Users_Group" />
		<property name="lastlogin" type="DateTime" />
		<property name="lastping" type="DateTime" />
		<property name="changepasswordkey" type="String" indexed="none">
			<constraint name="maxSize" max="50" />
		</property>
	</properties>
</document>