<div class="row">

	<div rbs-aside-column>
		<rbs-default-asides-for-editor id="rbsWebsitePageDefaultAsides"></rbs-default-asides-for-editor>
	</div>
	<div rbs-main-column>
		<div class="document-form">
			<rbs-page-header sub-title="{{ i18nAttr(modelKey('Rbs_Theme_Template'), ['ucf']) }}"></rbs-page-header>

			<rbs-workspace-config>
				<rbs-breadcrumb-config>
					<rbs-location href="(= 'Rbs_Theme'|rbsURL:'home' =)">{{ i18n('m.rbs.theme.admin.module_name', ['ucf']) }}</rbs-location>
					<rbs-location href="(= 'Rbs_Theme_Template'|rbsURL:'list' =)">{{ i18n('m.rbs.theme.admin.template_list', ['ucf']) }}</rbs-location>
				</rbs-breadcrumb-config>
			</rbs-workspace-config>

			<rbs-document-editor rbs-document-editor-rbs-theme-template=""></rbs-document-editor>
		</div>
	</div>

</div>