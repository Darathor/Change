<div class="row" ng-controller="Rbs_Theme_Template_ListController">

	<div rbs-aside-column>
		<rbs-default-asides-for-list></rbs-default-asides-for-list>
	</div>
	<div rbs-main-column>
		<rbs-page-header sub-title="{{ i18nAttr('m.rbs.theme.admin.template_list', ['ucf']) }}"></rbs-page-header>
		<rbs-document-filter-panel open-by-default="false"
				model="Rbs_Theme_Template" filter="filter" ></rbs-document-filter-panel>

		<rbs-document-list data-dlid="DLThemeTemplate1" model="Rbs_Theme_Template"
			activable="true" correction="true" filter-collection="filter" parent-property="theme">
			<rbs-column name="label" primary="true"></rbs-column>
		</rbs-document-list>
	</div>
</div>