<div class="row">

	<rbs-workspace-config menu="Rbs_Stock">
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_Stock'|rbsURL:'home' =)">{{ i18n('m.rbs.stock.admin.module_name', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_Stock_InventoryEntry'|rbsURL:'list' =)">{{ i18n('m.rbs.stock.admin.inventoryentry_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-full-width>
		<rbs-page-header title="{{ i18nAttr('m.rbs.stock.admin.inventoryentry_list', ['ucf']) }}"></rbs-page-header>
		<rbs-document-filter-panel open-by-default="false"
				model="Rbs_Stock_InventoryEntry" filter="filter" ></rbs-document-filter-panel>

		<div class="add-elements-bar">
			<a href ng-href="(= 'Rbs_Stock_InventoryEntry' | rbsURL:'new' =)">{{ i18n('m.rbs.stock.admin.inventoryentry_create', ['ucf']) }}</a>
		</div>

		<rbs-document-list data-dlid="DLRbsStockInventoryEntry"
				model="Rbs_Stock_InventoryEntry" filter-collection="filter">
			<rbs-column name="sku" sort="sku.code" primary="true"><a ng-href="(= doc | rbsURL =)">(= doc.sku.label =)</a></rbs-column>
			<rbs-column name="level"></rbs-column>
		</rbs-document-list>
	</div>
</div>