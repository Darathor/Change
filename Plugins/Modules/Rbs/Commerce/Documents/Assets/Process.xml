<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd"
	editable="true" activable="true">
	<properties>
		<property name="paymentConnectors" type="DocumentArray" document-type="Rbs_Payment_Connector" />
		<property name="shippingModes" type="DocumentArray" document-type="Rbs_Shipping_Mode" />
		<property name="taxBehavior" type="Integer" default-value="0" />
		<property name="modifiersOrderData" type="JSON" />
		<property name="sendMailOrderConfirm" type="Boolean" default-value="true"/>
		<property name="sendMailOrderCanceled" type="Boolean" default-value="true"/>
		<property name="sendMailShipmentPreparation" type="Boolean" default-value="true"/>
		<property name="sendMailShipmentFinalized" type="Boolean" default-value="true"/>
	</properties>
</document>