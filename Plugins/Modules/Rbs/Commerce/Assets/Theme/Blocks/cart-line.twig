		<button type="button" class="btn btn-small" onclick="script{{ blockId }}.deleteLine(this)" value="{{ cartLine.key }}">Supprimer</>
		<button type="button" class="btn btn-small" onclick="script{{ blockId }}.updateLine(this)" value="{{ cartLine.key }}" data-quantity="{{ cartLine.quantity }}">Mettre Ã  jour</>
	</td>
</tr>

