	<td>{{ cartLine.designation }}</td>
	<td>{{ formatPrice(cartLine.unitPriceValue) }}</td>
	<td>{{ cartLine.quantity }}</td>
	<td>{{ formatPrice(cartLine.priceValue) }}</td>
	<td>{{ formatPrice(cartLine.priceValueWithTax) }}</td>
