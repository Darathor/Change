<script type="text/ng-template" id="/simpleLine.static.tpl">
	<td>(= line.index+1 =)</td>
	<td data-ng-if="line.options.visualThumbnailSrc.Rbs_Commerce_Cart && line.options.url">
		<a href="javascript:;" data-ng-href="(= line.options.url =)" class="pull-left" >
			<img data-ng-src="(= line.options.visualThumbnailSrc.Rbs_Commerce_Cart =)" class="media-object" />
		</a>
	</td>
	<td data-ng-if="line.options.visualThumbnailSrc.Rbs_Commerce_Cart && !line.options.url">
		<img data-ng-src="(= line.options.visualThumbnailSrc.Rbs_Commerce_Cart =)" class="media-object" />
	</td>
	<td data-ng-if="!line.options.visualThumbnailSrc.Rbs_Commerce_Cart"></td>
	<td data-ng-if="line.options.url">
		<a href="javascript:;" data-ng-href="(= line.options.url =)">
		(= line.designation =)
		</a>
		<dl class="dl-inline-items" data-ng-if="line.options.axesInfo" >
			<dt data-ng-repeat-start="axeInfo in line.options.axesInfo">(= axeInfo.name =)</dt><dd data-ng-repeat-end="">(= axeInfo.value =)</dd>
		</dl>
	</td>
	<td data-ng-if="!line.options.url">
		(= line.designation =)
		<dl class="dl-inline-items" data-ng-if="line.options.axesInfo" data-ng-repeat="axeInfo in line.options.axesInfo">
			<dt>(= axeInfo.name =)</dt><dd>(= axeInfo.value =)</dd>
		</dl>
	</td>
	{% if parameters.displayPrices or parameters.displayPricesWithTax %}
		<td class="price">
			{% if parameters.displayPrices %}
				<del data-ng-if="line.basedAmount">(= line.options.formattedUnitBasedAmount =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.without_tax') }}">{{ i18n('m.rbs.catalog.front.without_tax_abbr') }}</abbr></del>
				<span>(= line.options.formattedUnitAmount =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.without_tax') }}">{{ i18n('m.rbs.catalog.front.without_tax_abbr') }}</abbr></span>
			{% endif %}
			{% if parameters.displayPrices and parameters.displayPricesWithTax %}<br />{% endif %}
			{% if parameters.displayPricesWithTax %}
				<del data-ng-if="line.basedAmount">(= line.options.formattedUnitBasedAmountWithTaxes =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.with_tax') }}">{{ i18n('m.rbs.catalog.front.with_tax_abbr') }}</abbr></del>
				<span>(= line.options.formattedUnitAmountWithTaxes =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.with_tax') }}">{{ i18n('m.rbs.catalog.front.with_tax_abbr') }}</abbr></span>
			{% endif %}
		</td>
	{% endif %}
	<td class="form-inline">
		<div class="form-group">
			<span data-ng-show="readonlyCart">(= line.quantity =)</span>
			<span data-ng-hide="readonlyCart" class="input-group" style="width: 100px;">
				<input type="number" min="0" step="1" required="true" data-ng-model="line.quantity" class="form-control" />
				<span class="input-group-btn">
					<button type="button" data-ng-disabled="!isQuantityEdited()" data-ng-click="updateLine(index)"
						class="btn btn-icon btn-default ng-class: {'btn-success': isQuantityEdited()}"
						title="{{ i18nAttr('m.rbs.commerce.front.update', ['ucf']) }}">
						<i class="glyphicon glyphicon-ok"></i>
					</button>
				</span>
			</span>
		</div>
	</td>
	{% if parameters.displayPrices or parameters.displayPricesWithTax %}
		<td class="price">
			{% if parameters.displayPrices %}
				<del data-ng-if="line.basedAmount">(= line.options.formattedBasedAmount =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.without_tax') }}">{{ i18n('m.rbs.catalog.front.without_tax_abbr') }}</abbr></del>
				<span>(= line.options.formattedAmount =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.without_tax') }}">{{ i18n('m.rbs.catalog.front.without_tax_abbr') }}</abbr></span>
			{% endif %}
			{% if parameters.displayPrices and parameters.displayPricesWithTax %}<br />{% endif %}
			{% if parameters.displayPricesWithTax %}
				<del data-ng-if="line.basedAmount">(= line.options.formattedBasedAmountWithTaxes =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.without_tax') }}">{{ i18n('m.rbs.catalog.front.without_tax_abbr') }}</abbr></del>
				<span>(= line.options.formattedAmountWithTaxes =) <abbr class="tax-mode" title="{{ i18nAttr('m.rbs.catalog.front.with_tax') }}">{{ i18n('m.rbs.catalog.front.with_tax_abbr') }}</abbr></span>
			{% endif %}
		</td>
	{% endif %}
	<td data-ng-hide="readonlyCart">
		<button type="button" class="btn btn-default btn-icon" data-ng-click="deleteLine(index)"
			title="{{ i18nAttr('m.rbs.commerce.front.remove', ['ucf']) }}">
			<i class="glyphicon glyphicon-remove"></i>
		</button>
	</td>
</script>

<div data-ng-show="loading" class="well text-center">
	<p>{{ i18n('m.rbs.commerce.front.please_wait_loading_cart', ['ucf', 'etc']) | raw }}</p>
	<img alt="" src="data:image/gif;base64," />
</div>
<div data-ng-hide="loading" data-ng-cloak="">
	<div class="alert alert-danger" data-ng-show="cart.errors.length">
		<ul class="list-unstyled">
			<li data-ng-repeat="error in cart.errors">(= error.message =)</li>
		</ul>
	</div>
	<div class="table-responsive">
		<table summary="{{ i18nAttr('m.rbs.commerce.front.cart_contents', ['ucf']) }}"
			class="table table-striped table-vertical-middle">
			<thead>
				<tr>
					<th>#</th>
					<th></th>
					<th>{{ i18n('m.rbs.commerce.front.designation', ['ucf']) }}</th>
					{% if parameters.displayPrices or parameters.displayPricesWithTax %}
						<th>{{ i18n('m.rbs.commerce.front.unit_price', ['ucf']) }}</th>
					{% endif %}
					<th>{{ i18n('m.rbs.commerce.front.quantity', ['ucf']) }}</th>
					{% if parameters.displayPrices or parameters.displayPricesWithTax %}
						<th>{{ i18n('m.rbs.commerce.front.total_price', ['ucf']) }}</th>
					{% endif %}
					<th data-ng-hide="readonlyCart">{{ i18n('m.rbs.commerce.front.actions', ['ucf']) }}</th>
				</tr>
			</thead>
			<tbody>
				<tr data-rbs-commerce-cart-line="" data-ng-repeat="(index, line) in cart.lines" data-cart="cart"
					data-line="line" data-index="index" data-original-quantities="original-quantities" data-loading="loading">
				</tr>
			</tbody>
		</table>
	</div>
	<div class="row">
		<div class="col-md-6">
			<table summary="{{ i18nAttr('m.rbs.commerce.front.taxes_amount', ['ucf']) }}"
				class="table table-striped table-bordered">
				<caption>{{ i18n('m.rbs.commerce.front.taxes_amount', ['ucf']) }}</caption>
				<tbody>
						<tr data-ng-repeat="tax in cart.context.formattedLinesTaxes">
							<td>(= tax.title =) ((= tax.formattedRate =))</td>
							<td class="price">(= tax.formattedValue =)</td>
						</tr>
				</tbody>
			</table>
		</div>
		<div class="col-md-6">
			<table summary="" class="table table-striped table-bordered">
				<tbody>
					<tr>
						<td>{{ i18n('m.rbs.commerce.front.amount_without_tax', ['ucf']) }}</td>
						<td class="price">(= cart.context.formattedLinesAmount =)</td>
					</tr>
					<tr>
						<td>{{ i18n('m.rbs.commerce.front.amount_with_tax', ['ucf']) }}</td>
						<td class="price">(= cart.context.formattedLinesAmountWithTaxes =)</td>
					</tr>
				</tbody>
			</table>
		</div>
	</div>

	<div data-ng-hide="readonlyCart" class="clearfix">
		<p>{{ i18n('m.rbs.commerce.front.accept_terms_and_conditions', ['ucf'], {'LINK_OPEN': "<a href=\"#{functionURL('Rbs_Commerce_TermsAndConditions')}\">", 'LINK_CLOSE': '</a>'})|raw }}</p>
		<p>
			<a class="btn btn-primary btn-lg pull-right" href="{{ functionURL('Rbs_Commerce_OrderProcess') }}" data-ng-disabled="!canOrder()">
				{{ i18n('m.rbs.commerce.front.proceed_to_checkout', ['ucf']) }}
			</a>
		</p>
	</div>
	<div data-ng-show="readonlyCart" class="clearfix">
		<p>
			<a class="btn btn-default pull-right" href="{{ functionURL('Rbs_Commerce_Cart') }}">
				{{ i18n('m.rbs.commerce.front.modify', ['ucf']) }}
			</a>
		</p>
	</div>
</div>