{% use 'Rbs_Commerce/cart-directives.twig' %}
{% if cartData %}
	<script type="text/javascript">
		window.__change['cartData'] = {{ cartData | json_encode | raw}};
	</script>

	{{ block('cartStructure') }}
	{{ block('cartLine') }}

	{% block cart %}
		<div data-rbs-commerce-cart="" data-cache-key="{{ blockId }}">
		</div>
	{% endblock %}
{% else %}
	{{ block('emptyCart') }}
{% endif %}