<div class="row">

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_Event'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.event.admin.module_name', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_Event_Category'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.event.admin.category_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Event_Category'), ['ucf']) }}"></rbs-page-header>
		<div ng-switch="editMode">
			<rbs-document-editor ng-switch-when="translate" rbs-document-editor-translate="" rbs-document-editor-rbs-event-category-translate=""></rbs-document-editor>
			<rbs-document-editor ng-switch-default="" rbs-document-editor-rbs-event-category=""></rbs-document-editor>
		</div>
	</div>

</div>