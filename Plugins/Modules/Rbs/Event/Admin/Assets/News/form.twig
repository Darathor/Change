<div class="document-form">
	<page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Event_News'), ['ucf']) }}"></page-header>

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Event'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.event.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Event_News'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.event.documents.news-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div ng-switch="editMode">
		<div ng-switch-when="translate" class="rbs-document-editor rbs-document-editor-translate rbs-document-editor-rbs-event-news-translate"></div>
		<div ng-switch-default="" class="rbs-document-editor rbs-document-editor-rbs-event-news"></div>
	</div>
</div>