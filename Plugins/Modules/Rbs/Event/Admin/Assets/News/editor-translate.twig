<form name="form" ng-submit="submit()" novalidate="">
	<rbs-field-text property="title" required="true"
		label="{{ i18nAttr('m.rbs.event.documents.news.title', ['ucf']) }}">
	</rbs-field-text>

	<hr />

	<rbs-field-rich-text property="summary" required="false" profile="Website"
		label="{{ i18nAttr('m.rbs.event.documents.news.summary', ['ucf']) }}">
	</rbs-field-rich-text>

	<rbs-field-rich-text property="text" required="false" profile="Website"
		label="{{ i18nAttr('m.rbs.event.documents.news.text', ['ucf']) }}">
	</rbs-field-rich-text>

	<hr />

	<rbs-field-date property="startPublication"
		label="{{ i18nAttr('m.rbs.event.documents.news.startPublication', ['ucf']) }}">
	</rbs-field-date>

	<rbs-field-date property="endPublication"
		label="{{ i18nAttr('m.rbs.event.documents.news.endPublication', ['ucf']) }}">
	</rbs-field-date>

	<form-button-bar></form-button-bar>
</form>