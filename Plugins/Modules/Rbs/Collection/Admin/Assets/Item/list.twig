<div data-ng-controller="Rbs_Collection_Item_ListController">
	<page-header title="{{ i18nAttr('m.rbs.collection.document.item-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Collection_Item" query="query" text-search="textSearch"></search-filters>
	<div>
		<a href="Rbs/Collection/Item/new">{{ i18n('m.rbs.collection.document.item-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLItem1"
	               publishable="true"
	               model="Rbs_Collection_Item"
	               query="query">
		<column name="label" primary="true"></column>
		<column name="collection"></column>
		<column name="value"></column>
	</rbs-document-list>
</div>