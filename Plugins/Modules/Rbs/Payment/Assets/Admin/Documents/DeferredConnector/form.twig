<div class="row">

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_Payment'|rbsURL:'home' =)">{{ i18n('m.rbs.payment.admin.module_name', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_Payment_Connector'|rbsURL:'list' =)">{{ i18n('m.rbs.payment.admin.connector_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Payment_DeferredConnector'), ['ucf']) }}"></rbs-page-header>
		<div ng-switch="editMode">
			<div ng-switch-when="translate" class="rbs-document-editor rbs-document-editor-translate rbs-document-editor-rbs-payment-deferred-connector-translate"></div>
			<div class="rbs-document-editor rbs-document-editor-rbs-payment-deferred-connector"></div>
		</div>
	</div>

</div>