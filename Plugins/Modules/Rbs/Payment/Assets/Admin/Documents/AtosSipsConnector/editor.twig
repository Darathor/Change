<form name="form" ng-submit="submit()" class="form-horizontal">
	<fieldset data-ng-show="section==''"
		data-form-section-label="{{ i18nAttr('m.rbs.admin.admin.general_properties', ['ucf']) }}"
		data-form-section-group="{{ i18nAttr('m.rbs.admin.admin.properties', ['ucf']) }}">

		<rbs-field-text property="label" required="true"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'label'), ['ucf']) }}">
		</rbs-field-text>

		<rbs-field-text property="code" required="true"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'code'), ['ucf']) }}">
		</rbs-field-text>

		<rbs-field-text property="title" required="true" rbs-sync-value="document.label"
			sync-title="{{ i18nAttr('m.rbs.generic.admin.synchronize_title_label', ['ucf']) }}"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'title'), ['ucf']) }}"
			input-class="form-control">
		</rbs-field-text>

		<rbs-field-rich-text property="description" required="false"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'description'), ['ucf']) }}">
		</rbs-field-rich-text>

		<rbs-field-picker property="visual"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'visual'), ['ucf']) }}"
			accepted-model="Rbs_Media_Image"
			selector-title="{{ i18nAttr('m.rbs.payment.admin.connector_select_visual', ['ucf']) }}">
		</rbs-field-picker>

		<rbs-field label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'cartFilterData'), ['ucf']) }}">
			<div rbs-document-filter-container="" filter="document.cartFilterData">
				<div rbs-cart-filter-definitions=""></div>
			</div>
		</rbs-field>
		<hr />

		<rbs-field-boolean property="processingMail"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'processingMail'), ['ucf']) }}">
		</rbs-field-boolean>

		<rbs-field-boolean property="successMail"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'successMail'), ['ucf']) }}">
		</rbs-field-boolean>

		<rbs-field-boolean property="errorMail"
			label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'errorMail'), ['ucf']) }}">
		</rbs-field-boolean>
	</fieldset>

	<fieldset data-ng-show="section=='technical'"
			data-form-section-label="{{ i18nAttr('m.rbs.payment.admin.technical_properties', ['ucf']) }}"
			data-form-section-group="{{ i18nAttr('m.rbs.admin.admin.properties', ['ucf']) }}">


		<rbs-field-text property="merchantId" required="true"
				label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'merchantId'), ['ucf']) }}"
				input-class="form-control">
		</rbs-field-text>

		<rbs-field-text property="merchantCountry" required="true"
				label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'merchantCountry'), ['ucf']) }}"
				input-class="form-control">
		</rbs-field-text>

		<rbs-field property="tpeParmcomContent"
				label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'tpeParmcomContent'), ['ucf']) }}">
				<textarea name="tpeParmcomContent" ng-model="document.tpeParmcomContent" class="form-control" rows="10"></textarea>
		</rbs-field>

		<rbs-field property="tpeCertifContent"
				label="{{ i18nAttr(propertyKey('Rbs_Payment_AtosSipsConnector', 'tpeCertifContent'), ['ucf']) }}">
			<textarea name="tpeCertifContent" ng-model="document.tpeCertifContent" class="form-control" rows="10"></textarea>
		</rbs-field>
	</fieldset>

	<fieldset
		data-ng-show="section=='systeminfo'"
		data-form-section-label="{{ i18nAttr('m.rbs.admin.adminjs.status', ['ucf']) }}"
		data-form-section-group="{{ i18nAttr('m.rbs.admin.admin.information', ['ucf']) }}"
		data-rbs-document-system-info-section="">
	</fieldset>

	<rbs-form-button-bar></rbs-form-button-bar>
</form>