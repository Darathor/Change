<div class="row">

	<rbs-workspace-config menu="Rbs_Order">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Order'|rbsURL:'home' =)">{{ i18n('m.rbs.order.admin.module_name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Order_Expedition'|rbsURL:'list' =)">{{ i18n('m.rbs.order.admin.expedition_list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-list></rbs-default-asides-for-list>
	</div>

	<div rbs-main-column>
		<page-header title="{{ i18nAttr('m.rbs.order.admin.expedition_list', ['ucf']) }}"></page-header>
		<search-filters model="Rbs_Order_Expedition" filter-query="query" text-search="textSearch"></search-filters>

		<div class="add-elements-bar">
			<a href ng-href="(= 'Rbs_Order_Expedition' | rbsURL:'new' =)">{{ i18n('m.rbs.order.admin.expedition_create', ['ucf']) }}</a>
		</div>

		<rbs-document-list data-dlid="DLRbsOrderExpedition"
				model="Rbs_Order_Expedition" filter-query="query">
			<column name="label" primary="true"></column>
			<column name="prepared">
				(= doc.prepared | rbsBoolean =)
			</column>
			<column name="shippingModeCode"></column>
			<column name="trackingCode"></column>
			<column name="carrierStatus"></column>
			<column name="itemCount"></column>
		</rbs-document-list>
	</div>

</div>