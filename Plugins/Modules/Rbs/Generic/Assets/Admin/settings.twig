<div class="row">

	<div rbs-aside-column>
		<rbs-default-asides-for-list></rbs-default-asides-for-list>
	</div>

	<div rbs-main-column>
		<h1>{{ i18n('m.rbs.generic.admin.initialize_title', ['ucf']) }}</h1>

		{% for structure in structures %}
			<h3><a href="{{ structure.href }}">{{ structure.title }}</a></h3>
			<p>{{ structure.description }}</p>
		{% endfor %}
	</div>

</div>