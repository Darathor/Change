<form name="form" data-ng-submit="submit()" class="form-horizontal">
	<fieldset data-ng-show="section==''"
		data-form-section-label="{{ i18nAttr('m.rbs.admin.admin.general_properties', ['ucf']) }}"
		data-form-section-group="{{ i18nAttr('m.rbs.admin.admin.properties', ['ucf']) }}">

		<rbs-field-text property="label" required="true"
			label="{{ i18nAttr(propertyKey('Rbs_Geo_TerritorialUnit', 'label'), ['ucf']) }}">
		</rbs-field-text>

		<rbs-field-text property="code" required="true"
			label="{{ i18nAttr(propertyKey('Rbs_Geo_TerritorialUnit', 'code'), ['ucf']) }}">
		</rbs-field-text>

		<rbs-field-text property="title" label="{{ i18nAttr(propertyKey('Rbs_Geo_TerritorialUnit', 'title'), ['ucf']) }}">
		</rbs-field-text>

		<rbs-field property="unitType" required="true"
				label="{{ i18nAttr(propertyKey('Rbs_Geo_TerritorialUnit', 'unitType'), ['ucf']) }}">
			<select ng-required="true" data-ng-model="document.unitType" rbs-items-from-collection="Rbs_Geo_Collection_UnitType"
				class="form-control">
				<option value=""> {{ i18n('m.rbs.geo.admin.select_unittype', ['ucf']) }}</option>
			</select>
		</rbs-field>

		<rbs-field-picker property="country" required="true" accepted-model="Rbs_Geo_Country"
			label="{{ i18nAttr(propertyKey('Rbs_Geo_TerritorialUnit', 'country'), ['ucf']) }}"
			selector-title="{{ i18nAttr('m.rbs.geo.admin.territorialunit_country_picker_title', ['ucf']) }}">
		</rbs-field-picker>

		<rbs-field-picker property="unitParent" accepted-model="Rbs_Geo_TerritorialUnit"
			label="{{ i18nAttr(propertyKey('Rbs_Geo_TerritorialUnit', 'unitParent'), ['ucf']) }}"
			selector-title="{{ i18nAttr('m.rbs.geo.admin.territorialunit_unitparent_picker_title', ['ucf']) }}">
		</rbs-field-picker>
	</fieldset>

	<fieldset data-ng-show="section=='systeminfo'"
		data-form-section-label="{{ i18nAttr('m.rbs.admin.adminjs.status', ['ucf']) }}"
		data-form-section-group="{{ i18nAttr('m.rbs.admin.admin.information', ['ucf']) }}"
		data-rbs-document-system-info-section="">
	</fieldset>

	<rbs-form-button-bar></rbs-form-button-bar>
</form>