	<page-header sub-title="{{ i18nAttr('m.rbs.geo.document.zone-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Geo_Zone" query="query" text-search="textSearch"></search-filters>

	<div>
		<a href="Rbs/Geo/Zone/new">{{ i18n('m.rbs.geo.document.zone-create', ['ucf']) }}</a>
