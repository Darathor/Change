<div>

	<rbs-workspace-config menu="Rbs_Geo">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Geo'|rbsURL:'home' =)">{{ i18n('m.rbs.geo.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Geo_Zone'|rbsURL:'list' =)">{{ i18n('m.rbs.geo.admin.js.zone-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<page-header title="{{ i18nAttr('m.rbs.geo.documents.zone-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Geo_Zone" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Geo_Zone' | documentURL:'new' =)">{{ i18n('m.rbs.geo.documents.zone-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLGeoZone" model="Rbs_Geo_Zone" filter-query="query">
		<column name="label" label="{{ i18nAttr('m.rbs.geo.documents.zone.label', ['ucf']) }}" primary="true"></column>
	</rbs-document-list>
</div>