	<document-list data-dlid="DLGeoZone" publishable="false" model="Rbs_Geo_Zone" query="query">
		<column name="label" label="{{ i18n('m.rbs.geo.document.zone.label', ['ucf']) }}" primary="true"></column>
	</document-list>
</div>