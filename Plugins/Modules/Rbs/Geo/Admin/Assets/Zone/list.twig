<div data-ng-controller="Rbs_Geo_Zone_ListController">
	<page-header title="{{ i18nAttr('m.rbs.geo.document.zone-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Geo_Zone" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Geo_Zone' | documentURL:'new' =)">{{ i18n('m.rbs.geo.document.zone-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLGeoZone" publishable="false" model="Rbs_Geo_Zone" filter-query="query">
		<column name="label" label="{{ i18n('m.rbs.geo.document.zone.label', ['ucf']) }}" primary="true"></column>
	</rbs-document-list>
</div>