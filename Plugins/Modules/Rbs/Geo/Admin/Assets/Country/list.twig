<div>

	<rbs-workspace-config menu="Rbs_Geo">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Geo'|rbsURL:'home' =)">{{ i18n('m.rbs.geo.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Geo_Country'|rbsURL:'list' =)">{{ i18n('m.rbs.geo.admin.js.country-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<page-header title="{{ i18nAttr('m.rbs.geo.document.country-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Geo_Country" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Geo_Country' | documentURL:'new' =)">{{ i18n('m.rbs.geo.document.country-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLGeoCountry" model="Rbs_Geo_Country" filter-query="query">
		<column name="label" label="{{ i18n('m.rbs.geo.document.country.label', ['ucf']) }}" primary="true"></column>
	</rbs-document-list>
</div>