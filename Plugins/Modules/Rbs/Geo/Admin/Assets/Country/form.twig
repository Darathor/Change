<div class="document-form">
	<page-header document="document" sub-title="{{ i18nAttr('m.rbs.geo.document.country', ['ucf']) }}"></page-header>

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Geo'|rbsURL:'home' =)">{{ i18n('m.rbs.geo.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Geo_Country'|rbsURL:'list' =)">{{ i18n('m.rbs.geo.admin.js.country-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div ng-switch="editMode">
		<div ng-switch-when="translate" class="rbs-document-editor rbs-document-editor-translate rbs-document-editor-rbs-geo-country-translate"></div>
		<div ng-switch-default="" class="rbs-document-editor rbs-document-editor-rbs-geo-country"></div>
	</div>
</div>