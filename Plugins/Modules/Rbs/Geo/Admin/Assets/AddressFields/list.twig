			<location href="(= 'Rbs_Geo'|rbsURL:'home' =)">{{ i18n('m.rbs.geo.admin.module_name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Geo_AddressFields'|rbsURL:'list' =)">{{ i18n('m.rbs.geo.admin.addressfields_list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
