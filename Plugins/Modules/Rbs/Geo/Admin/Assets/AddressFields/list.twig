	<rbs-document-list data-dlid="DLGeoAddressFields" model="Rbs_Geo_AddressFields" filter-query="query">
		<column name="label" label="{{ i18n(propertyKey('Rbs_Geo_AddressFields', 'label'), ['ucf']) }}" primary="true"></column>
