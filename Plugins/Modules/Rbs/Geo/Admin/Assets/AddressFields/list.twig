	<page-header title="{{ i18nAttr('m.rbs.geo.admin.addressfields_list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Geo_AddressFields" filter-query="query" text-search="textSearch"></search-filters>

	<div class="add-elements-bar">
		<a href ng-href="(= 'Rbs_Geo_AddressFields' | documentURL:'new' =)">{{ i18n('m.rbs.geo.admin.addressfields_create', ['ucf']) }}</a>
