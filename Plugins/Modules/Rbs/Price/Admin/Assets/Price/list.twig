<div>

	<rbs-workspace-config menu="Rbs_Price">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Price'|rbsURL:'home' =)">{{ i18n('m.rbs.price.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Price_Price'|rbsURL:'list' =)">{{ i18n('m.rbs.price.admin.js.price-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<page-header title="{{ i18nAttr('m.rbs.price.admin.js.price-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Price_Price" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Price_Price' | documentURL:'new' =)">{{ i18n('m.rbs.price.admin.js.price-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLRbsPricePrice" model="Rbs_Price_Price"  filter-query="query">
		<column name="boValue" primary="true">(=doc.formattedBoValue=)</column>
		<column name="boDiscountValue">(=doc.formattedBoDiscountValue=)</column>
		<column name="sku" sort="sku.code">(=doc.sku.code=)</column>
		<column name="webStore" sort="webStore.label">(=doc.webStore.label=)</column>
	</rbs-document-list>

</div>