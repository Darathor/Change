<div data-ng-controller="Rbs_Price_BillingArea_ListController">
	<page-header title="{{ i18nAttr('m.rbs.price.admin.js.billingarea-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Price_BillingArea" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Price_BillingArea' | documentURL:'new' =)">{{ i18n('m.rbs.price.admin.js.billingarea-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLRbsPriceBillingArea"
	               model="Rbs_Price_BillingArea"
	               filter-query="query">
		<column name="label" primary="true"></column>
		<column name="currencyCode">(= doc.currencyCode =)</column>
	</rbs-document-list>
</div>