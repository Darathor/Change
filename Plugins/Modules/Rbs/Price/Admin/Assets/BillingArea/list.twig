<div>

	<rbs-workspace-config menu="Rbs_Price">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Price'|rbsURL:'home' =)">{{ i18n('m.rbs.price.admin.module_name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Price_BillingArea'|rbsURL:'home' =)">{{ i18n('m.rbs.price.admin.billingarea_list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<page-header title="{{ i18nAttr('m.rbs.price.admin.billingarea_list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Price_BillingArea" filter-query="query" text-search="textSearch"></search-filters>

	<div class="add-elements-bar">
		<a href ng-href="(= 'Rbs_Price_BillingArea' | documentURL:'new' =)">{{ i18n('m.rbs.price.admin.billingarea_create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLRbsPriceBillingArea"
	               model="Rbs_Price_BillingArea"
	               filter-query="query">
		<column name="label" primary="true"></column>
		<column name="currencyCode">(= doc.currencyCode =)</column>
	</rbs-document-list>
</div>