<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd" 
	icon="folder" editable="true" activable="true">
	<properties>
		<property name="label" stateless="true" />
		<property name="product" type="Document" document-type="Rbs_Catalog_AbstractProduct" required="true" />
		<property name="webStore" type="Document" document-type="Rbs_Store_WebStore" required="true" />
		<property name="billingArea" type="Document" document-type="Rbs_Price_BillingArea" required="true" />
		<property name="targetId" type="DocumentId" />
		<property name="thresholdMin" type="Integer" required="true" default-value="1" />
		<property name="priority" type="Integer" required="true" default-value="25" />

		<property name="taxCategories" type="JSON" required="true" />
		<property name="value" type="Decimal" required="true" />
		<property name="valueWithoutDiscount" type="Decimal" />
		<property name="discountDetail" type="String" />
		<property name="ecoTax" type="Decimal" />

		<property name="boEditWithTax" type="Boolean" default-value="false" />
		<property name="boValue" type="Decimal" />
		<property name="boDiscountValue" type="Decimal" />
	</properties>
</document>