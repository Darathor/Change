<form name="form" ng-submit="submit()" class="form-horizontal">
	<fieldset data-ng-show="section==''"
		data-form-section-label="{{ i18nAttr('m.rbs.admin.admin.general_properties', ['ucf']) }}"
		data-form-section-group="{{ i18nAttr('m.rbs.admin.admin.properties', ['ucf']) }}">

		<rbs-field-text property="label" required="true" label="{{ i18nAttr(propertyKey('Rbs_Price_Fee', 'label'), ['ucf']) }}">
		</rbs-field-text>

		<rbs-field-picker property="sku" label="{{ i18nAttr(propertyKey('Rbs_Price_Fee', 'sku'), ['ucf']) }}"
				required="true"
				accepted-model="Rbs_Stock_Sku"
				selector-title="{{ i18nAttr('m.rbs.admin.admin.select', ['ucf']) }}"
				allow-creation="true"
				allow-edition="true">
		</rbs-field-picker>

		<rbs-field label="{{ i18nAttr(propertyKey('Rbs_Price_Fee', 'cartFilterData'), ['ucf']) }}">
			<div rbs-document-filter-container="" filter="document.cartFilterData">
				<div rbs-cart-fee-definitions=""></div>
			</div>
		</rbs-field>

		<rbs-field-picker property="shippingMode" label="{{ i18nAttr(propertyKey('Rbs_Price_Fee', 'shippingMode'), ['ucf']) }}"
				accepted-model="Rbs_Shipping_Mode"
				selector-title="{{ i18nAttr('m.rbs.admin.admin.select', ['ucf']) }}"
				allow-creation="true"
				allow-edition="true">
		</rbs-field-picker>
	</fieldset>

	<fieldset
		data-ng-show="section=='systeminfo'"
		data-form-section-label="{{ i18nAttr('m.rbs.admin.adminjs.status', ['ucf']) }}"
		data-form-section-group="{{ i18nAttr('m.rbs.admin.admin.information', ['ucf']) }}"
		data-document-system-info-section="">
	</fieldset>

	<rbs-form-button-bar></rbs-form-button-bar>
</form>
