<div class="row">
	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_Price'|rbsURL:'home' =)">{{ i18n('m.rbs.price.admin.module_name', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_Price_BillingArea'|rbsURL:'list' =)">{{ i18n('m.rbs.price.admin.billingarea_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Price_BillingArea'), ['ucf']) }}"></rbs-page-header>
		<div ng-switch="editMode">
			<rbs-document-editor ng-switch-when="translate" rbs-document-editor-translate="" rbs-document-editor-rbs-price-billing-area-translate=""></rbs-document-editor>
			<rbs-document-editor ng-switch-default="" rbs-document-editor-rbs-price-billing-area=""></rbs-document-editor>
		</div>
	</div>
</div>