<div class="row">
	<div data-rbs-aside-column="">
		{% include '@Rbs_Admin/view/asides.twig' %}
		{% include '@Rbs_Admin/view/links.twig' %}
	</div>

	<div data-rbs-main-column="">
		<rbs-page-header title="{{ i18nAttr('m.rbs.seo.admin.documentseo_list', ['ucf']) }}"></rbs-page-header>
		<rbs-document-filter-panel open-by-default="false" filter="filter"
			model="Rbs_Seo_DocumentSeo"></rbs-document-filter-panel>

		<div class="add-elements-bar">
			{% for newDocumentLink in attributes['newDocumentLinks'] %}
				<a href="javascript:;" data-ng-href="(= '{{ newDocumentLink.modelName }}' | rbsURL:'new' =)">
					{{ newDocumentLink.label }}
				</a>
			{% endfor %}
		</div>

		<rbs-document-list data-dlid="DLRbsSeoDocumentSeo" model="Rbs_Seo_DocumentSeo"
			correction="true" filter-collection="filter">
			<rbs-column name="label" primary="true"
				label="{{ i18n(propertyKey('Rbs_Seo_DocumentSeo', 'target'), ['ucf']) }}"></rbs-column>
			<rbs-column
				name="targetModelLabel" label="{{ i18n('m.rbs.seo.admin.documentseo_target_model', ['ucf']) }}"></rbs-column>
		</rbs-document-list>
	</div>
</div>