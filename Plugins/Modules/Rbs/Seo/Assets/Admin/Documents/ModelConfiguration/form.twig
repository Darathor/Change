<div class="row">

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_Seo'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.seo.admin.module_name', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_Seo_ModelConfiguration'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.seo.documents.modelconfiguration_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Seo_ModelConfiguration'), ['ucf']) }}"></rbs-page-header>
		<div ng-switch="editMode">
			<rbs-document-editor ng-switch-when="translate" rbs-document-editor-translate="" rbs-document-editor-rbs-seo-model-configuration-translate="" ></rbs-document-editor>
			<rbs-document-editor ng-switch-default="" rbs-document-editor-rbs-seo-model-configuration=""></rbs-document-editor>
		</div>
	</div>

</div>