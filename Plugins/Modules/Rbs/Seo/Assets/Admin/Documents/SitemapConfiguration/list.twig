<div class="row">

	<rbs-workspace-config menu="Rbs_Seo">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Seo'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.seo.admin.module_name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Seo_SitemapConfiguration'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.seo.documents.sitemapconfiguration_list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-list></rbs-default-asides-for-list>
	</div>

	<div rbs-main-column>
		<page-header title="{{ i18nAttr('m.rbs.seo.documents.sitemapconfiguration_list', ['ucf']) }}"></page-header><br />
		<rbs-document-filter-panel open-by-default="false"
				model="Rbs_Website_Website" filter="filter" ></rbs-document-filter-panel>

		<rbs-document-list data-dlid="DLCollection1" model="Rbs_Website_Website" filter-collection="filter" actions="">
			<quick-actions></quick-actions>
			<column name="label" primary="true" label="{{ i18n(propertyKey('Rbs_Website_Website', 'label'), ['ucf']) }}">
				<strong><a href="" ng-href="(= doc | rbsURL =)?section=sitemap">(= doc.label =)</a></strong>
			</column>
			<column name="sitemapGeneration" label="{{ i18n(propertyKey('Rbs_Website_Website', 'sitemapGeneration'), ['ucf']) }}">
				(= doc.sitemapGeneration | rbsBoolean =)
			</column>
		</rbs-document-list>
	</div>

</div>