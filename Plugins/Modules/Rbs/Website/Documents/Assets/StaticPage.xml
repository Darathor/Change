<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd"
	extends="Rbs_Website_Page" tree-name="Rbs_Website" publishable="true">
	<properties>
		<property name="title" required="true">
			<dboptions length="100" />
			<constraint name="maxSize" max="80" />
		</property>
		<property name="section" type="Document" document-type="Rbs_Website_Section" required="true" />
		<property name="hideLinks" type="Boolean" />

		<property name="displayDocument" type="Document" />
		<property name="publicationSections" type="DocumentArray" stateless="true" internal="true" />
	</properties>
</document>