<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd"
		  extends="Rbs_Website_Page" frontoffice-indexable="true" has-url="true" abstract="false" tree-name="Rbs_Website">
	<properties>
		<property name="section" type="Document" document-type="Rbs_Website_Section" required="true"/>
		<property name="pathPart" type="String" localized="true" has-correction="true"/>
		<property name="title" required="true">
			<dboptions length="100"/>
			<constraint name="maxSize" max="80"/>
		</property>

		<property name="metaTitle" type="LongString" localized="true" has-correction="true"/>
		<property name="description" type="LongString" localized="true" has-correction="true"/>
		<property name="keywords" type="LongString" localized="true" has-correction="true"/>

		<property name="robotsMeta" type="String" default-value="index,follow" indexed="none">
			<dboptions length="20"/>
		</property>
	</properties>
</document>