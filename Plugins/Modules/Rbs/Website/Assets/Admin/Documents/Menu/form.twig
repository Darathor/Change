<div class="row">

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Simpleform'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.simpleform.admin.module_name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Simpleform_Form'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.simpleform.admin.form_list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
	</div>

	<div class="document-form" rbs-main-column>
		<page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Website_Menu'), ['ucf']) }}"></page-header>

		<rbs-workspace-config>
			<rbs-breadcrumb-config>
				<location href="(= 'Rbs_Website'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.website.admin.module_name', ['ucf']) }}</location>
				<location href="(= 'Rbs_Website_Menu'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.website.admin.menu_list', ['ucf']) }}</location>
			</rbs-breadcrumb-config>
		</rbs-workspace-config>

		<div class="rbs-document-editor rbs-document-editor-rbs-website-menu"></div>
	</div>

</div>