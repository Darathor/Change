<form name="form" ng-submit="submit()" novalidate="">
	<rbs-field-text property="title" required="true"
		label="{{ i18nAttr(propertyKey('Rbs_Website_FunctionalPage', 'title'), ['ucf']) }}">
	</rbs-field-text>

	<rbs-structure-editor name="contents" property="editableContent" ng-model="document.editableContent"
		template="(=pageTemplate=)">
		<button type="button" class="btn btn-default" rbs-fullscreen-toggle=""><i class="icon-resize-full"></i> {{ i18nAttr('m.rbs.website.documents.functionalpage.full-screen', ['ucf']) }}</button>
	</rbs-structure-editor>

	<rbs-form-button-bar></rbs-form-button-bar>
</form>