<div class="row">

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
		<a rbs-aside-other-link href="(= document | rbsURL:'tree' =)">{{ i18n('m.rbs.website.admin.website_go_to_tree', ['ucf']) | raw }}</a>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Website_Website'), ['ucf']) }}"></rbs-page-header>
		<div ng-switch="editMode">
			<rbs-document-editor ng-switch-when="translate" rbs-document-editor-translate="" rbs-document-editor-rbs-website-website-translate=""></rbs-document-editor>
			<rbs-document-editor ng-switch-default="" rbs-document-editor-rbs-website-website=""></rbs-document-editor>
		</div>
	</div>

</div>