<div>

	<rbs-workspace-config menu="Rbs_Website">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Website'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.website.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Website_Website'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.website.document.website-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<page-header title="{{ i18nAttr('m.rbs.website.document.website-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Website_Website" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Website_Website' | documentURL:'new'=)">{{ i18n('m.rbs.website.document.website-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLWebsite1"
	               model="Rbs_Website_Website"
	               filter-query="query"
	               actions="default">
		<column name="label" primary="true">
			<strong><a href ng-href="(= doc|documentURL:'tree'=)">(= doc.label =)</a></strong>
			<i class="icon-rbs-navigate-to"></i>
		</column>
		<column name="baseurl">
			<a href ng-href="(= doc.baseurl =)" target="_blank">(= doc.baseurl =) <i class="icon-external-link-sign"></i></a>
		</column>
	</rbs-document-list>

</div>