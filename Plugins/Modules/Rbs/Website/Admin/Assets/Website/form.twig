<div class="document-form">
	<page-header document="document" sub-title="{{ i18nAttr('m.rbs.website.documents.website', ['ucf']) }}"></page-header>

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Website'|rbsURL:'home' =)">{{ i18nAttr('m.rbs.website.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Website_Website'|rbsURL:'list' =)">{{ i18nAttr('m.rbs.website.documents.website-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div ng-switch="editMode">
		<div ng-switch-when="translate" class="rbs-document-editor rbs-document-editor-translate rbs-document-editor-rbs-website-website-translate"></div>
		<div ng-switch-default="" class="rbs-document-editor rbs-document-editor-rbs-website-website"></div>
	</div>
</div>