<div>

	<rbs-workspace-config menu="Rbs_Store">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Store'|rbsURL:'home' =)">Boutiques</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<page-header title="{{ i18nAttr('m.rbs.store.document.webstore-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Store_WebStore" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Store_WebStore' | documentURL:'new' =)">{{ i18n('m.rbs.store.document.webstore-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLRbsStoreWebStore" model="Rbs_Store_WebStore" filter-query="query">
		<column name="label" primary="true"></column>
		<column name="billingAreas">
			<span class="label" ng-repeat="ba in doc.billingAreas">
				<a href ng-href="(= ba | documentURL =)">(= ba.label =)</a>
			</span>
		</column>
	</rbs-document-list>
</div>