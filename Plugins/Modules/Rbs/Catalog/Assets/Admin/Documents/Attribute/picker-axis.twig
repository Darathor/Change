<div data-ng-controller="rbsAxisAttributesSelector">
	<rbs-document-list data-dlid="DLRbsAxisAttributesSelector" model="{{ query.model }}" load-query="axisAttributesQuery"
		filter-query="query" extend="picker" actions="">
		<rbs-column name="label" primary="true">
			{% if (query.multiple) %}
			<span class="pull-right">
				<button type="button" class="btn btn-xs" data-ng-click="extend.replaceWithDocument(doc)">
					<i class="icon-arrow-right" title="{{ i18nAttr('m.rbs.admin.admin.replace_by_this', ['ucf']) }}"></i>
				</button>
			</span>
			{% endif %}
			<a href="javascript:;" data-ng-click="extend.selectDocument(doc)">(= doc.label =)</a>
		</rbs-column>
	</rbs-document-list>
</div>
