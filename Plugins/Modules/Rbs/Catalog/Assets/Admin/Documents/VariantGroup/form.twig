<div class="row">

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_Catalog'|rbsURL:'home' =)">{{ i18n('m.rbs.catalog.admin.module_name', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_Catalog_VariantGroup'|rbsURL:'list' =)">{{ i18n('m.rbs.catalog.admin.variantgroup_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
		<rbs-aside ng-if="! document.isNew()" template="Document/Rbs/Catalog/VariantGroup/aside.twig"></rbs-aside>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Catalog_VariantGroup'), ['ucf']) }}"></rbs-page-header>
		<div class="rbs-document-editor rbs-document-editor-rbs-catalog-variant-group"></div>
	</div>

</div>