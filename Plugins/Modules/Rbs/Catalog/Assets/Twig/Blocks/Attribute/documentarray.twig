{% for document in item.value %}
	{% set model = document.getDocumentModel() %}
	{% if document.getDocumentModelName() == 'Rbs_Media_Image' %}
		<img src="{{ catalogMacros.attributeThumbnailURL(document) }}" alt="{{ document.alt }}" />
	{% elseif model.isPublishable() %}
		<a href="{{ canonicalURL(item.value) }}">
			{% if model.isLocalized() %}{{ document.currentLocalization.title }}{% else %}{{ document.title }}{% endif %}
		</a>
	{% elseif model.hasProperty('title') %}
		{% if model.isLocalized() %}{{ document.currentLocalization.title }}{% else %}{{ document.title }}{% endif %}
	{% endif %}
{% endfor %}