<div>
	<rbs-workspace-config menu="Rbs_Catalog">
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Catalog'|rbsURL:'home' =)">{{ i18n('m.rbs.catalog.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Catalog_Listing'|rbsURL:'list' =)">{{ i18n('m.rbs.catalog.document.listing-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<page-header sub-title="{{ i18nAttr('m.rbs.catalog.document.listing-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Catalog_Listing" filter-query="query" text-search="textSearch"></search-filters>

	<div>
		<a href ng-href="(= 'Rbs_Catalog_Listing' | documentURL:'new' =)">{{ i18n('m.rbs.catalog.document.listing-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLRbsCatalogListing" model="Rbs_Catalog_Listing" activable="true"
		filter-query="query" modification-date="false">
		<quick-actions>
			<a href="(= doc | documentURL:'productcategorizations' =)">{{ i18nAttr('m.rbs.catalog.document.listing.products') }}</a>
			|| [action default]
		</quick-actions>

		<column name="label" primary="true"></column>
	</rbs-document-list>
</div>