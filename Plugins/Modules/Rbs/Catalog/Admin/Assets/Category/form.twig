<div class="document-form">
	<page-header document="document" sub-title="{{ i18nAttr('m.rbs.catalog.document.category', ['ucf']) }}"></page-header>

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Catalog'|rbsURL:'home' =)">{{ i18n('m.rbs.catalog.admin.js.module-name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Catalog_Category'|rbsURL:'list' =)">{{ i18n('m.rbs.catalog.document.category-list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div ng-switch="editMode">
		<div ng-switch-when="translate" class="rbs-document-editor rbs-document-editor-translate rbs-document-editor-rbs-catalog-category-translate"></div>
		<div ng-switch-default="" class="rbs-document-editor rbs-document-editor-rbs-catalog-category"></div>
	</div>
</div>