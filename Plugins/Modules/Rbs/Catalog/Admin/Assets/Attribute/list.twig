<div data-ng-controller="Rbs_Catalog_Attribute_ListController">
	<page-header title="{{ i18nAttr('m.rbs.catalog.admin.js.attribute-list', ['ucf']) }}"></page-header>
	<search-filters model="Rbs_Catalog_Attribute" filter-query="query" text-search="textSearch"></search-filters>
	<div>
		<a href ng-href="(= 'Rbs_Catalog_Attribute' | documentURL:'new' =)">{{ i18n('m.rbs.catalog.admin.js.attribute-create', ['ucf']) }}</a>
	</div>

	<rbs-document-list data-dlid="DLRbsCatalogAttribute" model="Rbs_Catalog_Attribute" filter-query="query">
		<column name="label" primary="true"></column>
		<column name="valueTypeFormatted" label="{{ i18nAttr('m.rbs.catalog.document.attribute.valuetype', ['ucf']) }}"></column>
	</rbs-document-list>
</div>