<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd"
	localized="true" editable="true" publishable="true">
	<properties>
		<property name="description" type="RichText" localized="true" />
		<property name="visuals" type="DocumentArray" document-type="Rbs_Media_Image" />
		<property name="pictograms" type="DocumentArray" document-type="Rbs_Media_Image" />
		<property name="brand" type="Document" document-type="Rbs_Brand_Brand" />
		<property name="code" type="String" />
		<property name="sku" type="Document" document-type="Rbs_Stock_Sku" />
		<property name="attribute" type="Document" document-type="Rbs_Catalog_Attribute" />
		<property name="attributeValues" type="JSON" />
		<property name="categorizable" type="Boolean" default-value="true" />
		<property name="declinationGroup" type="Document" document-type="Rbs_Catalog_DeclinationGroup" />
		<property name="newSkuOnCreation" type="Boolean" stateless="true" />
	</properties>
</document>