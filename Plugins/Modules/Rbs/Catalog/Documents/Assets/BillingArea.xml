<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd" 
	icon="folder" editable="true" tree-name="Rbs_Catalog">
	<properties>
		<property name="code" type="String">
			<dboptions length="80" />
			<constraint name="unique" />
		</property>
		<property name="currency" type="Document" document-type="Rbs_Catalog_Currency" required="true" />
		<property name="boEditWithTax" type="Boolean" default-value="false" />
		<property name="storePriceWithTax" type="Boolean" default-value="false" />
		<property name="taxes" type="DocumentArray" document-type="Rbs_Catalog_Tax" />
		<property name="taxesData" type="JSON" stateless="true" />
	</properties>
</document>