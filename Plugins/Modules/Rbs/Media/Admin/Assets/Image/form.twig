<div class="document-form">
	<page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_Media_Image'), ['ucf']) }}"></page-header>

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<location href="(= 'Rbs_Media'|rbsURL:'home' =)">{{ i18n('m.rbs.media.admin.module_name', ['ucf']) }}</location>
			<location href="(= 'Rbs_Media_Image'|rbsURL:'list' =)">{{ i18n('m.rbs.media.admin.image_list', ['ucf']) }}</location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div ng-switch="editMode">
		<div ng-switch-when="translate" class="rbs-document-editor rbs-document-editor-translate rbs-document-editor-rbs-media-image-translate"></div>
		<div ng-switch-default="" class="rbs-document-editor rbs-document-editor-rbs-media-image"></div>
	</div>
</div>