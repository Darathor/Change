<div class="row">

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_User'|rbsURL:'home' =)">{{ i18n('m.rbs.user.admin.module_name', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_User_User'|rbsURL:'list' =)">{{ i18n('m.rbs.user.admin.user_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
		<a rbs-aside-other-link rbs-active-route="" href ng-href="(= document|rbsURL:'applications' =)">{{ i18n('m.rbs.user.admin.applications_aside_link', ['ucf']) | raw }}</a>
		<a rbs-aside-other-link rbs-active-route="" href ng-href="(= document|rbsURL:'permission' =)">{{ i18n('m.rbs.user.admin.permissions_aside_link', ['ucf']) | raw }}</a>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_User_User'), ['ucf']) }}"></rbs-page-header>
		<div class="rbs-document-editor rbs-document-editor-rbs-user-user"></div>
	</div>

</div>