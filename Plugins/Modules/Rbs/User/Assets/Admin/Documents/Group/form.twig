<div class="row">

	<rbs-workspace-config>
		<rbs-breadcrumb-config>
			<rbs-location href="(= 'Rbs_User'|rbsURL:'home' =)">{{ i18n('m.rbs.user.admin.user_list', ['ucf']) }}</rbs-location>
			<rbs-location href="(= 'Rbs_User_Group'|rbsURL:'list' =)">{{ i18n('m.rbs.user.admin.group_list', ['ucf']) }}</rbs-location>
		</rbs-breadcrumb-config>
	</rbs-workspace-config>

	<div rbs-aside-column>
		<rbs-default-asides-for-editor></rbs-default-asides-for-editor>
		<a rbs-aside-other-link rbs-active-route="" href="javascript:;" ng-href="(= document | rbsURL:'groupUsers' =)">{{ i18n('m.rbs.user.admin.users_in_group_aside_link', ['ucf']) | raw }}</a>
	</div>

	<div class="document-form" rbs-main-column>
		<rbs-page-header document="document" sub-title="{{ i18nAttr(modelKey('Rbs_User_Group'), ['ucf']) }}"></rbs-page-header>
		<div class="rbs-document-editor rbs-document-editor-rbs-user-group"></div>
	</div>

</div>