{
	"change:clear-cache": {
		"description": "Clear Project File Cache"
	},
	"change:compile-documents": {
		"description": "Compile Documents"
	},
	"change:compile-plugins-registration": {
		"description": "Compile Plugins Registration"
	},
	"change:create-command": {
		"description": "Create an empty console command",
		"dev": true,
		"arguments":{
			"package": {
				"description": "name of the package (vendor/module or change)",
				"required": true,
				"default": null
			},
			"cmdname": {
				"description": "name of the command (e.g. my-cmd)",
				"required": true,
				"default": null
			}
		}
	},
	"change:generate-db-schema": {
		"description": "Generate database Schema",
		"options":{
			"with-modules": {
				"description": "Compile Modules Db schema",
				"shortcut": "m"
			}
		}
	},
	"change:set-document-root": {
		"description": "Initialize documents root files",
		"arguments":{
			"path": {
				"description": "Path of document root (e.g. \".\")",
				"required": true,
				"default": null
			},
			"resourcePath": {
				"description": "static resource base url",
				"required": false,
				"default": "/Assets"
			}
		}
	},
	"change:initialize-model": {
		"description": "Initialize an new XML model file",
		"dev": true,
		"arguments":{
			"vendor": {
				"description": "vendor of the target module",
				"required": true,
				"default": null
			},
			"module": {
				"description": "name of the target module",
				"required": true,
				"default": null
			},
			"name": {
				"description": "short name of the model",
				"required": true,
				"default": null
			}
		}
	},
	"change:initialize-plugin": {
		"description": "Initialize an new empty plugin skeleton",
		"dev": true,
		"options":{
			"package": {
				"description": "package of plugin",
				"default": null,
				"shortcut": "p"
			}
		},
		"arguments":{
			"type": {
				"description": "type of plugin",
				"required": true,
				"default": null
			},
			"vendor": {
				"description": "vendor of the plugin",
				"required": true,
				"default": null
			},
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	},
	"change:install-package": {
		"description": "Install a Package",
		"options":{
			"vendor": {
				"description": "vendor of the package",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the package",
				"required": true,
				"default": null
			}
		}
	},
	"change:install-plugin": {
		"description": "Install a plugin",
		"options":{
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	},
	"change:register-plugin": {
		"description": "Register a plugin or all plugins",
		"options":{
			"all": {
				"description": "register all available plugins",
				"shortcut": "a"
			},
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": false
			}
		}
	},
	"change:disable-plugin": {
		"description": "Disable a plugin",
		"options":{
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	},
	"change:enable-plugin": {
		"description": "Enable a plugin",
		"options":{
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	},
	"change:deregister-plugin": {
		"description": "Deregisters a registered plugin",
		"options":{
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	},
	"change:deinstall-plugin": {
		"description": "Deinstall a plugin",
		"options":{
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	}
}