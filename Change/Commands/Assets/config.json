{
	"change:clear-cache": {
		"description": "Clear Project File Cache"
	},
	"change:compile-documents": {
		"description": "Compile Documents"
	},
	"change:compile-plugins-registration": {
		"description": "Compile Plugins Registration"
	},
	"change:create-command": {
		"description": "Create an empty console command",
		"dev": true,
		"arguments":{
			"package": {
				"description": "name of the package (vendor/module or change)",
				"required": true,
				"default": null
			},
			"cmdname": {
				"description": "name of the command (e.g. my-cmd)",
				"required": true,
				"default": null
			}
		}
	},
	"change:generate-db-schema": {
		"description": "Generate database Schema",
		"options":{
			"with-modules": {
				"description": "Compile Modules Db schema",
				"shortcut": "m"
			}
		}
	},
	"change:set-document-root": {
		"description": "Initialize documents root files",
		"arguments":{
			"path": {
				"description": "Path of document root  (e.g. \".\")",
				"required": true,
				"default": null
			}
		}
	},
	"change:initialize-model": {
		"description": "Initialize an new XML model file",
		"dev": true,
		"arguments":{
			"vendor": {
				"description": "vendor of the target module",
				"required": true,
				"default": null
			},
			"module": {
				"description": "name of the target module",
				"required": true,
				"default": null
			},
			"name": {
				"description": "short name of the model",
				"required": true,
				"default": null
			}
		}
	},
	"change:initialize-plugin": {
		"description": "Initialize an new empty plugin skeleton",
		"dev": true,
		"options":{
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	},
	"change:install-package": {
		"description": "Install a Package",
		"options":{
			"vendor": {
				"description": "vendor of the package",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the package",
				"required": true,
				"default": null
			}
		}
	},
	"change:install-plugin": {
		"description": "Install a plugin",
		"options":{
			"type": {
				"description": "type of plugin",
				"default": "module",
				"shortcut": "t"
			},
			"vendor": {
				"description": "vendor of the plugin",
				"default": "Project",
				"shortcut": "e"
			}
		},
		"arguments":{
			"name": {
				"description": "short name of the plugin",
				"required": true,
				"default": null
			}
		}
	},
	"change:register-plugins": {
		"description": "Register All Plugins"
	}
}